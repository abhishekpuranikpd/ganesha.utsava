generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model DhoopaDeeparatiRegistration {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  fullName  String
  mobile    String
  address   String
  createdAt DateTime @default(now())
  videoUrl  String?
}
